{
  "name": "@tinyhelpdesk/demo-plugin",
  "version": "0.0.4",
  "description": "Demo plugin for the TinyHelpdesk",
  "author": {
    "name": "Bj√∂rn Torben Hahn",
    "email": "info@bjoerntorben.de"
  },
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com:tinyhelpdesk/demo-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/tinyhelpdesk/demo-plugin/issues"
  },
  "homepage": "https://github.com/tinyhelpdesk/demo-plugin#readme",
